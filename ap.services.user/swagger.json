{
  "swagger": "2.0",
  "info": {
    "title": "APJS User Service",
    "version": "v1"
  },
  "paths": {
    "/getUser": {
      "get": {
        "produces": ["application/json"],
        "parameters": [{
            "name": "id",
            "in": "query",
            "required": true,
            "type": "number"
        }],
        "responses": {
          "200": {
            "description": "200 response"
          },
          "400": {
            "description": "400 response"
          },
          "404": {
            "description": "200 response"
          }
        }
      }
    },
    "/getUsers": {
      "get": {
        "produces": ["application/json"],
        "responses": {
          "200": {
            "description": "200 response"
          },
          "404": {
            "description": "404 response"
          }
        }
      }
    },
    "/addUser": {
        "post": {
          "produces": ["application/json"],
          "parameters": [
            {
              "in": "body",
              "name": "user",
              "required": true,
              "schema": {
                "type": "object",
                "properties": {
                  "id": {
                    "type": "number",
                    "required": true
                  },
                  "firstName": {
                    "type": "string",
                    "required": true
                  },
                  "middleName": {
                    "type": "string",
                    "required": false
                  },
                  "lastName": {
                    "type": "string",
                    "required": true
                  },
                  "userName": {
                    "type": "string",
                    "required": true
                  },
                  "password": {
                    "type": "string",
                    "required": true
                  },
                  "lastModified": {
                    "type": "number",
                    "required": true
                  },
                  "isActive": {
                    "type": "boolean",
                    "required": true
                  }
                }
              }
            }
          ],
          "responses": {
            "200": {
              "description": "200 response"
            }
          }
        }
      }
  },
  "consumes": ["application/json"]
}
